<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".adminViewConsumerListActivity"
    android:background="@drawable/eee"
    >

    <LinearLayout
        android:id="@+id/loConsumerList"
        android:layout_width="312dp"
        android:layout_height="376dp"
        android:layout_marginEnd="36dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.733"></LinearLayout>

    <TextView
        android:id="@+id/textView4"

        android:layout_width="376dp"
        android:layout_height="72dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"


        android:background="@color/btn_logut_bg"
        android:fontFamily="@font/allerta_stencil"
        android:text="Consumers List"
        app:layout_constraintBottom_toTopOf="@+id/loConsumerList"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="4dp" />
</android.support.constraint.ConstraintLayout>